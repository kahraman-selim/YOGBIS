@model IEnumerable<YOGBIS.Common.VModels.KomisyonlarVM>

<div class="card mb-3">
    <div class="card-header">
        <i class="fas fa-table mr-1"></i>
        <b>Komisyon Sorumluları</b>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered table-hover table-striped display responsive nowrap" id="myTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th class="text-center">SN</th>
                        <th class="text-center">Personel Adı Soyadı</th>
                        <th class="text-center">Sorumlu Olduğu Komisyonlar</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        var sirano = 0;
                    }
                    @if (Model != null)
                    {
                        @foreach (var item in Model)
                        {
                            sirano++;
                            <tr>
                                <td>@sirano</td>

                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>


